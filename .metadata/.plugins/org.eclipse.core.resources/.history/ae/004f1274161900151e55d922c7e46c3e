package com.example.projectofinal;

public class RegistroDB {
	
	Integer id;
	String nombre;
	String direccion;
	String observaciones;
	String foto;
	String telefono;
	Float rate;
	
	public RegistroDB(Integer id, String nombre, String direccion, 
	String observaciones, String foto, String telefono, Float rate) {
		
		this.id = id;
		this.nombre = nombre;
		this.direccion = direccion;
		this.observaciones = observaciones;
		this.foto = foto;
		this.telefono = telefono;
		this.rate = rate;
	}

}
